-- Med safes
for i,v in pairs(game:GetService("Workspace").Map.BredMakurz:GetDescendants()) do
	if string.find(v.Name,"MediumSafe") then 
	    if v:FindFirstChild("MainPart") then 
            ESP:Add(v.MainPart, {
                Name = "Medium Safe",
                IsEnabled = "mediumSafe",
                Color = Color3.fromRGB(139, 203, 255)
            })
        end 
	end 
end

-- Small safes
