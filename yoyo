local Toggle4 = Section6:CreateToggle("Infinite Stamina", nil, function(v) 

    local StaminaTake = getrenv()._G.S_Take
    local StaminaFunc = getupvalue(StaminaTake, 2)
    
    for i,v in pairs(getupvalues(StaminaFunc)) do
        if type(v) == "function" and getinfo(v).name == "Upt_S" then
            local oldFunc; 
            oldFunc = hookfunction(v, function(...)
                getupvalue(StaminaFunc, 6).S = 100
                return oldFunc(...)
            end)
        end
    end
end)

local SilentAimLabel = Section6:CreateLabel("Anti(s)")

local Toggle1 = Section6:CreateToggle("Anti fall damage", nil, function(State)
    _G.NoFallDamage = State
    local OldNameCall = nil
    OldNameCall = hookmetamethod(game, "__namecall", function(Self, ...)
        if not checkcaller() and getnamecallmethod() == "FireServer" and Self.Name == "__DFfDD" and _G.NoFallDamage then
            return
        end
        return OldNameCall(Self, ...)
    end)
    end)
    Toggle1:CreateKeybind("J", function(Key)
        print(Key)
    end)

    local Toggle1 = Section6:CreateToggle("Anti Wire Damage", nil, function(State)
        _G.NoBarbRagdollFallDamage = State
            local OldNameCall = nil
            OldNameCall = hookmetamethod(game, "__namecall", function(Self, ...)
                if not checkcaller() and getnamecallmethod() == "FireServer" and Self.Name == "BHHh" and _G.NoBarbRagdollFallDamage then
                    return
                end
                return OldNameCall(Self, ...)
            end)
        end)

        local Toggle1 = Section6:CreateToggle("Anti Ragdoll", Settings.NoRagdoll, function(V)
			Settings.NoRagdoll = V
		end)

        local Toggle1 = Section6:CreateToggle("Anti Stomp", false, function(V)
			_G.AntiStomp = V
			local Downed = game:GetService("ReplicatedStorage").CharStats[game:GetService("Players").LocalPlayer.Name].Downed
			Downed.Changed:Connect(function()
				if Downed.Value == true and _G.AntiStomp == true then
					for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
						if v:IsA("BasePart") then
							v:Destroy()
						end
					end
				end
			end)
		end)


        local Toggle6 = Section6:CreateToggle("Anti Flash", nil, function(v)
            _G.NoFlashBang = v
            game.Workspace.Camera.ChildAdded:Connect(function(Item)
                if _G.NoFlashBang == true then
                    if Item.Name == "BlindEffect" then
                        Item.Enabled = false
                    end
                end
            end)
        
            game.Players.LocalPlayer.PlayerGui.ChildAdded:Connect(function(Item)
                if Item.Name == "FlashedGUI" then
                    if _G.NoFlashBang == true then
                        Item.Enabled = false
                    end
                end
            end)
        end)
        
        local Toggle6 = Section6:CreateToggle("Anti Smoke", nil, function(v)
            _G.NoSmoke = v
        game.Workspace.Debris.ChildAdded:Connect(function(Item)
            if Item.Name == "SmokeExplosion" then
                if _G.NoSmoke == true then
                    wait(0.1)
                    Item.Particle1:Destroy()
                    Item.Particle2:Destroy()
                end
            end
        end)
        
        game.Players.LocalPlayer.PlayerGui.ChildAdded:Connect(function(Item)
            if Item.Name == "SmokeScreenGUI" then
                if _G.NoSmoke == true then
                    Item.Enabled = false
                end
            end
        end)
        end)



        local SilentAimLabel = Section6:CreateLabel("Noclips")
        local Toggle6 = Section6:CreateToggle("Noclip", nil, function(v) 
            _G.Noclip = v
            
            local function Noclip()
            if game.Players.LocalPlayer.Character ~= nil and _G.Noclip == true then
            for _, selfChar in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if selfChar:IsA("BasePart") and selfChar.CanCollide == true and selfChar.Name then
                        selfChar.CanCollide = false
                    end
                end
            end
            end
            
            game:GetService('RunService').Stepped:Connect(Noclip)
            end)

            local Toggle1 = Section6:CreateToggle("Noclip Doors", nil, function(State)
                _G.Noclipdoor = State
        
                if  _G.Noclipdoor == true then
                    for _, v in pairs(game:GetService("Workspace").Map.Doors:GetChildren()) do
                        if v:FindFirstChild("DoorBase") then
                            v.DoorBase.CanCollide = false
                        end
                        if v:FindFirstChild("DoorA") then
                            v.DoorA.CanCollide = false
                        end
                        if v:FindFirstChild("DoorB") then
                            v.DoorB.CanCollide = false
                        end
                        if v:FindFirstChild("DoorC") then
                            v.DoorC.CanCollide = false
                        end
                        if v:FindFirstChild("DoorD") then
                            v.DoorD.CanCollide = false
                        end
                    end
                else
                    for _, v in pairs(game:GetService("Workspace").Map.Doors:GetChildren()) do
                        if v:FindFirstChild("DoorBase") then
                            v.DoorBase.CanCollide = true
                        end
                        if v:FindFirstChild("DoorA") then
                            v.DoorA.CanCollide = true
                        end
                        if v:FindFirstChild("DoorB") then
                            v.DoorB.CanCollide = true
                        end
                        if v:FindFirstChild("DoorC") then
                            v.DoorC.CanCollide = true
                        end
                        if v:FindFirstChild("DoorD") then
                            v.DoorD.CanCollide = true
                        end
                    end
                end
            end)
    Toggle6:CreateKeybind("Y", function(Key)
        print(Key)
    end)

    local SilentAimLabel = Section6:CreateLabel("Unlocks")
    local Toggle6 = Section6:CreateToggle("Unlock Nearby Doors", Settings.UnlockDoorsNearby, function(V)
        Settings.UnlockDoorsNearby = V
    end)

    local Toggle6 = Section6:CreateToggle("Open Nearby Doors", Settings.OpenDoorsNearby, function(V)
        Settings.OpenDoorsNearby = V
    end)

